{
  "name": "tomhart/laravel-resilient-http-client",
  "description": "Laravel Resilient Http Client",
  "keywords": [
    "tomhart",
    "http",
    "client",
    "resilient"
  ],
  "homepage": "https://github.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Hart",
      "email": "tomharto_@hotmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.4",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^3.8 || ^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "TomHart\\HttpClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TomHart\\HttpClient\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TomHart\\HttpClient\\ResilientServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html=build"
  }
}
